                                   VerticalAlignment="Stretch"
                                   HorizontalContentAlignment="Right"
                                   VerticalContentAlignment="Center"
                                   Content="Contact" />
                            <TextBox Name="txtBillContact"
                                     Grid.Row="4"
                                     Grid.Column="1"
                                     Grid.ColumnSpan="5"
                                     Height="23"
                                     Margin="3"
                                     HorizontalAlignment="Stretch"
                                     VerticalAlignment="Center" />
