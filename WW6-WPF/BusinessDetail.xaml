        <TextBlock Grid.Row="1"
                   Grid.RowSpan="2"
                   Grid.Column="2"
                   Margin="0,0,326,399"
                   HorizontalAlignment="Right"
                   Text="Name:" />
        <TextBox Name="txtName"
                 Grid.Row="1"
                 Grid.Column="2"
                 Grid.ColumnSpan="2"
                 Height="23"
                 Margin="191,0,0,0"
                 HorizontalAlignment="Stretch"
                 VerticalAlignment="Top"
                 Text="{Binding Path=curBus.Bus_Name}" />
    </Grid>
</UserControl>
